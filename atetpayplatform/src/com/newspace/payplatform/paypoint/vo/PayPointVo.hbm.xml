<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>												   					
	<class name="com.newspace.payplatform.paypoint.vo.PayPointVo" table="t_payplatform_paypoint">
		<id name="id" >
			<column name="id" />
			<generator class="sequence">
				<param name="sequence">seq_payplatform_paypoint_id</param>
			</generator>
		</id>
		
		<property name="appId" type="string"/>
		<property name="payCode" column="payPoint" type="string"/>
		<property name="payName" type="string"/>
		<property name="money" type="double"/>
		<property name="state"/>
		<property name="syncTime"/>
	</class>
</hibernate-mapping>
