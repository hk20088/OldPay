<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>												   					
	<class name="com.newspace.payplatform.bindtelephone.vo.BindTelephoneVo" table="t_payplatform_bindTel">
		<id name="id" >
			<column name="id" />
			<generator class="sequence">
				<param name="sequence">seq_payplatform_bindTel_id</param>
			</generator>
		</id>
		
		<property name="userId" type="integer"/>
		<property name="atetId" type="string"/>
		<property name="productId" type="string"/>
		<property name="deviceCode" type="string"/>
		<property name="telephone" type="string"/>
		<property name="deviceType" type="integer"/>
		<property name="state" type="integer"/>
		<property name="ext" type="string"/>
		<property name="lastSendTime" type="timestamp"/>
		<property name="sendCount" type="integer"/>
		
	</class>
</hibernate-mapping>
